{
  "name": "nuxt-ts-graphql-serverless-docker",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "fe:dev": "MY_ENV=dev nuxt",
    "fe:build": "nuxt build",
    "fe:generate": "nuxt generate",
    "fe:start": "nuxt start",
    "be:dev": "nodemon -e js,ts --ignore node_modules --exec ts-node server.ts",
    "be:start": "ts-node server.ts",
    "start:sls": "MY_ENV=sls nuxt build && NUXT_TELEMETRY_DISABLED=1 MY_ENV=sls serverless offline start --noPrependStageInUrl",
    "start:docker": "npm run fe:start & npm run be:start"
  },
  "license": "MIT",
  "dependencies": {
    "@nuxt/types": "^2.15.3",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/vuetify": "^1.11.3",
    "@types/express": "^4.17.11",
    "apollo-boost": "^0.4.9",
    "apollo-server-express": "^2.21.1",
    "cross-fetch": "^3.0.6",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "nuxt": "^2.15.3",
    "nuxt-property-decorator": "^2.9.1",
    "serverless-http": "^2.7.0",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "serverless": "^2.29.0",
    "serverless-offline": "^6.8.0"
  }
}
